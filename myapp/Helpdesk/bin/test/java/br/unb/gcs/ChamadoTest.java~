package br.unb.gcs;

import br.unb.gcs.model.Chamado;
import br.unb.gcs.model.Solucao;
import br.unb.gcs.model.Status;
import br.unb.gcs.model.TipoChamado;
import br.unb.gcs.model.Usuario;

import org.junit.Test;

public class ChamadoTest {

	@Test
	public void test() {
		
		DateTime dataInicial = new DateTime();
		DateTime dataFinal = new DateTime();
		Chamado chamado = new Chamado();
		Usuario solicitante = new Usuario("lmatos","102030","Luiz","luiz@gmail.com","0900300");
		Usuario tecnico = new Usuario("pchaves","123456","Pedro","luiz@gmail.com","0101101");
		Status status = new Status(1,"Fechado");
		Solucao solucao = new Solucao(1,chamado,"Reparo no monitor",dataInicial.plusDays(1));
		TipoChamado tipochamado = new TipoChamado(1,"Tipo Chamado");
		
		chamado.setId(1);
		chamado.setDataInicial(dataInicial.plusDays(1));
		chamado.setDataFinal(dataFinal.plusDays(1));
		chamado.setDescricao("Chamado Test");
		chamado.setSolicitante(solicitante);
		chamado.setTecnico(tecnico);
		chamado.setTipoChamado(tipochamado);
		chamado.setLocalEquipamento("FGA LAB-I6");
		chamado.setPatrimonio(2939934);
		
		
	}

}
